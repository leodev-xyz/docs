
import doc, fmt from require "leadoc.doc"
import stringify from require "leadoc.json"


file = ...

local content
with io.open file, "r"
    content = \read "*a"
    \close!

if ".yue" == file\sub -4, -1
    import to_lua from require "yue"
    content  = to_lua content

doc = doc (load content)!, fmt

export json = stringify doc.tree

